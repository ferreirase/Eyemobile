{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error",
      "no-console": "off",
      "implicit-arrow-linebreak": "off",
      "function-paren-newline": "off",
      "comma-dangle": "off"
    }
}
